<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="classes" name="Suite">

	<listeners>
		<listener class-name="newmaven.TestComponents.ListenersClass" />
	</listeners>
	<test thread-count="5" name="Submit Order">
		<classes>
			<class name="newmaven.Tests.submitTest" />
		</classes>
	</test>
	<test thread-count="5" name="Error Validations">
		<classes>
			<class name="newmaven.Tests.errorMessageValidations" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
